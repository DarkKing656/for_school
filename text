void checkNewline(const char* line) {
    int hasNewline = 0;

    // Проверяем каждый символ строки
    for (int i = 0; i < strlen(line); i++) {
        if (line[i] == '\n') {
            hasNewline = 1; // Обнаружен символ "\n"
            break;
        }
    }

    if (!hasNewline) {
        printf("\n"); // Выводим символ новой строки
    }
}
